{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "ApimServiceName": {
        "type": "string"
      }
    },
    "resources": [
        {
            "type": "Microsoft.ApiManagement/service/subscriptions",
            "name": "[concat(parameters('ApimServiceName'), '/master')]",
            "apiVersion": "2018-01-01",
            "scale": null,
            "properties": {
                "userId": "[resourceId('Microsoft.ApiManagement/service/users', parameters('ApimServiceName'), '/1')]",
                "productId": "[resourceId('Microsoft.ApiManagement/service/products', parameters('ApimServiceName'), 'unlimited')]",
                "displayName": "Built-in all-access subscription",
                "state": "active"
            }
        }
    ]
}